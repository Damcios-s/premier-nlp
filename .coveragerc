[run]
source = agents, services, data_classes, tools, config
relative_files = true
omit = 
    */test/*
    */__pycache__/*
    */venv/*
    */env/*
    */.venv/*
    */site-packages/*
    main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    pass

[html]
directory = htmlcov

[xml]
output = coverage.xml